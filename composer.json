      "name": "Frederic Dewinne",
      "email": "frederic@continuousphp.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^4.6",
    "squizlabs/php_codesniffer": "^2.3",
    "behat/behat": "^3.0"
  },
  "require": {
    "php": ">=5.4.0",
    "phing/phing": "~2.9",
    "continuousphp/sdk": "~0.1"
  },
  "autoload": {
    "psr-4": {
