{
  "name": "continuousphp/phing-tasks",
  "homepage": "https://github.com/continuousphp/phing-tasks",
  "description": "Phing tasks for continuousphp",
  "type": "library",
  "keywords": ["build", "tool", "task", "phing", "continuousphp"],
  "license": "Apache-2.0",
  "authors": [
